  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="https://www.digisnare.com/images/slider/digisnare-technologies-team.png" class="d-block w-100" alt="Slide 1">
    </div>
    <div class="carousel-item">
      <img src="https://www.digisnare.com/images/slider/award-winning.png" class="d-block w-100" alt="Slide 2">
    </div>
    <div class="carousel-item">
      <img src="https://www.digisnare.com/images/slider/digisnare-technologies-team.png" class="d-block w-100" alt="Slide 3">
    </div>
